# Local Development Environment Variables
# This file is for local testing without API keys

# Use mock stories for local development
VITE_USE_MOCK_STORIES=true

# Supabase (can use dummy values for local testing)
VITE_SUPABASE_URL=https://dummy.supabase.co
VITE_SUPABASE_ANON_KEY=dummy_key_for_local_testing

# Stripe (use test keys or dummy values)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_dummy_for_local_testing

# OpenAI (leave empty to trigger mock generator)
VITE_OPENAI_API_KEY=

# Enable dev features
VITE_DEV_MODE=true

# Story generation version
VITE_STORYGEN_V2_ENABLED=false